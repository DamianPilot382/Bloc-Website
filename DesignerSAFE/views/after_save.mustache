<div class="popup shadow-layer-1">
  <a class="close shadow-layer-1" id="closePopup" href="#">x</a>
  <div>
    <h2>{{localization.popup_message}}</h2>
  </div>
  <div class="content">
    <div class="material-input-box active f-row">
      <div class="material-placeholder-mask">{{localization.link}}</div>
      <a id="link" class="material-main-input" target="_blank"></a>
    </div>
    {{#model_name}}
      <div class="material-input-box f-row active">
        <div class="material-placeholder-mask">{{localization.save_form.model_name}}</div>
        <input disabled="disabled" type="text" class="material-main-input model-name" value="{{model_name}}"/>
      </div>
    {{/model_name}}
  </div>
</div>